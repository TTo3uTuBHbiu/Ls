class Box {
    int width; // ������ �������
    int height; // ������ �������
    int depth; // ������� �������

    // �����������
    Box(int a, int b) {
        width = a;
        height = b;
        depth = 10;
    }

    // ��������� ����� �������
    int getVolume() {
    	return width * height * depth;
    }
}
